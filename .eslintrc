{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "plugins": [
        "react"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
      },
    "rules": {
        "react/prop-types": 0,
        "camelcase": 0,
        "dot-notation": 0,
        "no-unused-expressions": 0
    }
}
